{
  "name" : "zend_firebase/zend_firebase",
  "description" : "PHP7 Firebase library",
  "version" : "v0.1.6",
  "require" : {
    "php" : ">=7.0",
    "guzzlehttp/guzzle" : "6.2.2",
    "guzzlehttp/psr7" : "1.3.1",
    "monolog/monolog" : "1.21.0"
  },
  "repositories" : [ {
    "type" : "composer",
    "url" : "https://packagist.org/"
  } ],
  "require-dev" : {
    "phpunit/PHPUnit" : "5.3.1",
    "squizlabs/php_codesniffer" : "2.7.0",
    "friendsofphp/php-cs-fixer" : "v1.12.3",
    "phpunit/php-code-coverage" : "3.3.x-dev"
  },
  "license" : "BSD-3-Clause",
  "keywords" : [ "firebase", "firebase library", "google firebase library", "php firebase", "php library firebase", "REST", "wrapper", "php7 firebase", "REST firebase", "REST php7 firebase" ],
  "autoload-dev" : {
    "psr-4" : {
      "ZendFirebaseTest\\" : "tests/"
    }
  },
  "minimum-stability" : "stable",
  "autoload" : {
    "psr-4" : {
      "ZendFirebase\\" : "src/"
    }
  },
  "scripts" : {
    "cs-check" : "phpcs",
    "cs-fix" : "phpcbf",
    "test" : "phpunit",
    "test-coverage" : "phpunit --coverage-clover clover.xml"
  },
  "support" : {
    "email" : "info@samuelventimiglia.it"
  },
  "homepage" : "http://www.samuelventimiglia.it/",
  "prefer-stable" : true,
  "authors" : [ {
    "name" : "Biasin Davide",
    "email" : "dav.biasin@gmail.com",
    "role" : "Web Developer"
  }, {
    "name" : "Samuel Ventimiglia",
    "email" : "info@samuelventimiglia.it",
    "role" : "Web Developer"
  } ]
}